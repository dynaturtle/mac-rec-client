		<dict>
			<key>CFBundleTypeExtensions</key>
			<array>
				<string>voicenote</string>
			</array>
			<key>CFBundleTypeIconFile</key>
			<string>AppIcon</string>
			<key>CFBundleTypeName</key>
			<string>VoiceNotes Document</string>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>LSHandlerRank</key>
			<string>Owner</string>
			<key>LSItemContentTypes</key>
			<array>
				<string>com.voicenotes.document</string>
			</array>
		</dict>
	</array>
	<key>UTExportedTypeDeclarations</key>
	<array>
		<dict>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.data</string>
				<string>public.content</string>
			</array>
			<key>UTTypeDescription</key>
			<string>VoiceNotes Document</string>
			<key>UTTypeIconFile</key>
			<string>AppIcon</string>
			<key>UTTypeIdentifier</key>
			<string>com.voicenotes.document</string>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>voicenote</string>
				</array>
				<key>public.mime-type</key>
				<array>
					<string>application/x-voicenotes-document</string>
				</array>
			</dict>
		</dict>
	</array>
	<key>NSServices</key>
	<array>
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Record with VoiceNotes</string>
			</dict>
			<key>NSMessage</key>
			<string>recordAudio</string>
			<key>NSPortName</key>
			<string>VoiceNotes</string>
			<key>NSReturnTypes</key>
			<array>
				<string>NSStringPboardType</string>
			</array>
			<key>NSSendTypes</key>
			<array>
				<string>NSStringPboardType</string>
			</array>
		</dict>
	</array>
