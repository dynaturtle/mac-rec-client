	<false/>
	<key>NSSupportsAutomaticGraphicsSwitching</key>
	<true/>
	<key>NSHighResolutionCapable</key>
	<true/>
	<key>LSMultipleInstancesProhibited</key>
	<true/>
	<key>NSAppleEventsUsageDescription</key>
	<string>VoiceNotes may use Apple Events to integrate with other applications for enhanced functionality.</string>
	<key>NSSystemAdministrationUsageDescription</key>
	<string>VoiceNotes may need system administration access for advanced audio configuration.</string>
